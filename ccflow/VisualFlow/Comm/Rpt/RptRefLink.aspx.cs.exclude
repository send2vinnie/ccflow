	/// </summary>
	public partial class UIRptRefLink : WebPage
	{
		public UCRefLink UCRefLink1
		{
			get
			{
				return (UCRefLink)this.FindControl("UCRefLink1");
			}
		}
		public string ClassName
		{
			get
			{
				string str= this.Request.QueryString["ClassName"];
				if (str==null)
					throw new Exception("没有接收到参数ClassName。");
				return str;
			}
		}
		public string Val
		{
			get
			{
				string str= this.Request.QueryString["val"];
				if (str==null)
					throw new Exception("没有接收到参数val。");
				return str;
			}
		}

		protected void Page_Load(object sender, System.EventArgs e)
		{
			this.UCRefLink1.BindDB(this.ClassName,this.Val);
		}

		#region Web 窗体设计器生成的代码
