<?xml version="1.0" encoding="UTF-8"?>
<!--
  有关如何配置 ASP.NET 应用程序的详细消息，请访问
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <configSections>
  </configSections>
  <system.web>
    <identity impersonate="false" userName="administrator" password="jiaozi" />
    <compilation debug="true" targetFramework="4.0">
      <assemblies>
        <add assembly="Microsoft.Office.Interop.Owc11, Version=11.0.0.0, Culture=neutral, PublicKeyToken=71E9BCE111E9429C" />
        <add assembly="Microsoft.Web.UI.WebControls, Version=1.0.2.226, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
      </assemblies>
    </compilation>
    <webServices>
      <protocols>
        <add name="HttpGet" />
        <add name="HttpPost" />
      </protocols>
    </webServices>
    <globalization requestEncoding="gb2312" responseEncoding="gb2312" />
    <httpHandlers>
      <add path="scissors.axd" verb="*" type="BitmapCutter.Core.HttpHandler.BitmapScissors,BitmapCutter.Core" validate="false" />
    </httpHandlers>
  </system.web>
  <appSettings>
    <!-- 是否:debug状态. 0 表示不是, 1 是，如果系统发布后，请将此修改成0，以提高执行效率。 -->
    <add key="IsDebug" value="0" />
    <!-- 数据库连接配置:  AppCenterDBType类型为Oracle,MSSQL,MySQL,DB2,Access  -->
    <add key="AppCenterDSN" value="server=anwg\sql2008;uid=sa;pwd=sa;initial catalog=ccflow;"/>
    <!--<add key="AppCenterDSN" value="Password=sa;Persist Security Info=True;User ID=sa;Initial Catalog=ccflow;Data Source=localhost\sqlexpress;Timeout=2" />-->
    <add key="AppCenterDBType" value="MSSQL" />
    <!-- 默认语言 CH 为简体中文,其它语言请参考枚举 -->
    <add key="SysLanguage" value="CH" />
    <!-- 初始化的数据库缓存池数量根据自己的服务器性能，与访问者多少的需要,设置相应的参数,以提高并发访问效率. -->
    <add key="InitConnNum" value="10" />
    <!-- 系统编号:不要修改它. -->
    <add key="SysNo" value="WF" />
    <add key="SysName" value="驰骋工作流程管理系统" />
    <!-- 每次取最大数据行数(不要修改) -->
    <add key="TopNum" value="99999" />
    <!-- 默认每页数据行数(不要低于5) -->
    <add key="PageSize" value="15" />
    <!--群集IP: 对于群集有效 -->
    <add key="CIP" value="127.0.0.1" />
    <!--单据ftp服务器配置:如果没有用到就可以忽略掉. -->
    <add key="FtpIP" value="192.168.1.100" />
    <add key="FtpUser" value="administrator" />
    <add key="FtpPass" value="jiaozi" />
    <!--有关考核信息设置:利用如下配置系统就可以获取每个节点的时效信息,可以把非工作时间排除在外. -->
    <add key="AMFrom" value="08:30" />
    <add key="AMTo" value="11:30" />
    <add key="PMFrom" value="13:00" />
    <add key="PMTo" value="17:30" />
    <!-- 周休息类型: 0双休,1单休,2不休-->
    <add key="WeekResetType" value="0" />
    <!-- 假期设置-->
    <add key="Holiday" value="10-01,10-02,10-03,10-04,10-05,05-01,05-02" />
    <!--服务信息: 当程序出现异常就会显示该信息,以方便通知管理员处理. -->
    <add key="ServiceMail" value="" />
    <add key="ServiceTel" value="" />
    <!-- 是否显示标题图片:对独立运行模式有效.  -->
    <add key="IsShowTitle" value="1" />
    <!-- 是否显启用风格: -->
    <add key="IsEnableStyle" value="1" />
    <!--是否提示流程数量 -->
    <add key="IsShowFlowNum" value="1" />
    <!-- 是否显示用户编号:对于英文用户有效 -->
    <add key="ShowUserNoOnly" value="0" />
    <!-- 辅应用数据Oracle连接一号库 -->
    <add key="DBAccessOfOracle9i1" value="" />
    <!-- 辅应用数据Oracle连接二号库 -->
    <add key="DBAccessOfOracle9i2" value="" />
    <!-- 辅应用数据Oracle连接三号库 -->
    <add key="DBAccessOfOracle9i3" value="" />
    <!-- 辅应用数据Oracle连接四号库 -->
    <add key="DBAccessOfOracle9i4" value="" />
    <!-- 辅应用数据MSSQL2000连接 -->
    <add key="DBAccessOfMSSQL2000" value="" />
    <!-- 辅应用数据 Ole 连接 -->
    <add key="DBAccessOfOLE" value="" />
    <!-- 辅应用数据 ODBC 连接 -->
    <add key="DBAccessOfODBC" value="" />
    <!--用来惟一标识用户，主要用来根据不同的用户显示不同的Logo-->
    <add key="CompanyID" value="CCFlow" />
    <!--FTP 相关的-->
    <add key="FTPServerIP" value="ftp://221.204.237.33" />
    <!-- 在FTP上的惟一标识,最好换成公司或个人的英文名-->
    <add key="UserIdentifier" value="CCFlow" />
    <add key="FTPUserEmail" value="test@gmail.com" />
    <add key="TemplateFtpConfig" value="server=127.0.0.1;username=awg;pwd=123456;flowroot=Flow.流程模板;formroot=Frm.表单模板;shareflowroot=Flow.流程模板/temp"/>
  </appSettings>
  <system.serviceModel>
    <bindings />
    <client />
  </system.serviceModel>
  <system.webServer>
    <handlers accessPolicy="Read, Execute, Script" />
    <defaultDocument>
      <files>
        <add value="login.htm" />
      </files>
    </defaultDocument>
  </system.webServer>
</configuration>